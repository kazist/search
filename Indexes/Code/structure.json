    "query": {
        "from": {
            "table": "Search\\Indexes\\Code\\Tables\\Indexes",
            "alias": "si",
            "name": "#__search_indexes"
        },
        "select": [
            "si.id",
            "si.title",
            "si.description",
            "si.subset_id",
            "ss.title AS subset_id_title",
            "si.record_id",
            "si.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "si.date_index",
            "si.published",
            "si.publish_up",
            "si.publish_down",
            "si.sale_price",
            "si.discounted_price",
            "si.created_by",
            "si.date_created",
            "si.modified_by",
            "si.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Search\\Subsets\\Code\\Tables\\Subsets",
                "alias": "ss",
                "name": "#__search_subsets",
                "on": "si.subset_id = ss.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "si.image = mm.id"
            }
        ]
    }
